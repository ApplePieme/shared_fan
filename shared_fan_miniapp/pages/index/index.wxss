/**index.wxss**/
.main{
    margin-top: 50%;
    display: flex;
    justify-content: center;
}
.btn{
    width: 460rpx;
    height: 80rpx;
    display: flex;
    justify-content: space-around;
    background-color: #34ebc3;
    border-radius: 30rpx;
    padding: 10rpx;
    box-shadow: 0px 0px 50px 10px #DDDDDD;
}
.btn image{
    width: 80rpx;
    height: 80rpx;
}
.btn text{
    color: #fff;
    font-size: 38rpx;
    display: flex;
    align-items: center;
}